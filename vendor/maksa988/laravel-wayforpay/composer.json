{
    "name": "maksa988/laravel-wayforpay",
    "description": "WayForPay payments for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "maksa988",
            "email": "maksa988ua@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "guzzlehttp/guzzle": "~6.2|~7.0",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0",
        "wayforpay/php-sdk": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.5|^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^6.0|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Maksa988\\WayForPay\\Facades\\": "src/Facades",
            "Maksa988\\WayForPay\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Maksa988\\WayForPay\\WayForPayServiceProvider"
            ],
            "aliases": {
                "WayForPay": "Maksa988\\WayForPay\\Facades\\WayForPay"
            }
        }
    }
}
